{
  "name": "tatatap.com-cli",
  "description": "Command line interface to save and retrieve notes from the tatatap.com note-taking service",
  "version": "0.9.3",
  "author": "joe k @1800joe",
  "bin": {
    "tap": "./bin/run"
  },
  "bugs": "https://github.com/thumblab/tatatap-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^3.0.1",
    "axios": "^0.19.2",
    "luxon": "^1.24.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/thumblab/tatatap-cli",
  "keywords": [
    "oclif"
  ],
  "license": "Apache 2.0",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "ttt",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "thumblab/tatatap-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
